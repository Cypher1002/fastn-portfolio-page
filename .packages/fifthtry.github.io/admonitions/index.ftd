ftd.color background-solid: $inherited.colors.info.base
optional string emoji:
children wrapper:

-- ftd.column:
color: $cbox.title-color
background.solid: $cbox.background-solid
padding-horizontal.px: 16
padding-vertical.px: 16
padding-right.px: 36
border-radius.px: 4
width if { ftd.device != "mobile" }: auto
width: fill-container
min-width.fixed.percent: 50
margin-top.px: 16
margin-bottom.px: 16

-- ftd.row:

-- emoji: $cbox.emoji

-- ftd.text: $cbox.title
role: $inherited.types.copy-regular
color: $cbox.title-color
margin-left.px: 8
style: bold

-- end: ftd.row

-- ftd.text:
if: { cbox.body != NULL }
role: $inherited.types.copy-regular
color: $cbox.text-color
padding-top.px: 8

$cbox.body

-- ftd.column:
width: fill-container
children: $cbox.wrapper

-- end: ftd.column

-- end: ftd.column

-- end: cbox










